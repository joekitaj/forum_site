<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Document working</title>
  </head>
  <body>
    <h1>Hello World</h1>
    <% posts.forEach((post) => { %>
      <div class="post">
        <h3><%= post.title %></h3>
        <p><%= post.content %></p>
        <p><%= post.likes %></p>
      </div>
    <% }) %>
  </body>
</html>
