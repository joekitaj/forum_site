<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Document working</title>
  </head>
  <body>
    <h1>Hello World</h1>
    <a href="/posts/new">New Post</a>
    <% posts.forEach((post) => { %>
      <a href="/posts/<%=post.id%>">
        <div class="post">
          <h3><%= post.title %></h3>
          <p><%= post.post_content %></p>
          <p>Likes: <%= post.likes %></p>
          <p>Comments: <%= post.comments %></p>
        </div>
      </a>
    <% }) %>
  </body>
</html>
